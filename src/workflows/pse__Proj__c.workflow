    <rules>
        <fullName>EcoHealthyStartNotification</fullName>
        <actions>
            <name>Eco_Healthy_Start_Email_Notification</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>EcoSetProjectHealthyStartHasRan</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>pse__Proj__c.ProjectReviewCategory__c</field>
            <operation>equals</operation>
            <value>C-0,C-1,C-2</value>
        </criteriaItems>
        <criteriaItems>
            <field>pse__Proj__c.HealthyStartHasRan__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>Emails the healthy start team at the start of a project for C-0,C-1 and C-2 projects</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>EcoHealthyStartReStartMarginNotify</fullName>
        <actions>
            <name>EcoMarginLossNotification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>pse__Proj__c.pse__Margin__c</field>
            <operation>lessThan</operation>
            <value>&quot;USD -500,000&quot;</value>
        </criteriaItems>
        <description>Emails the healthy start team each time the Margin threshold breaches 500K</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
