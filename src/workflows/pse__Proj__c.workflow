    <rules>
        <fullName>Eco Change Manager Updated Resilience</fullName>
        <actions>
            <name>ChangeManagerResilienceIsChanged</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>ChangeManagerResilienceUpdatedBy</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>ChangeManagerResilienceUpdatedDate</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>OR(ISCHANGED(projclosResilienceCompleteExplantation__c),
    ISCHANGED( projcloseResilienceComplete__c),
    ISCHANGED( PrimaryCityName__c )

)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
