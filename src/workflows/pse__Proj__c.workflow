    <rules>
        <fullName>EcoProjectSetupCompletedNotification</fullName>
        <actions>
            <name>EcoProjectSetupSucess</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(pse__Stage__c),
ISPICKVAL(pse__Stage__c,&quot;Execution&quot;),
ISPICKVAL(PriorValue(pse__Stage__c),&quot;Planning&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>EcoRequired Security Documentation Task</fullName>
        <actions>
            <name>ACTION_Complete_Required_Security_Documentation</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>pse__Proj__c.pse__Stage__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>EcoSetDistributionRuleToCostWork</fullName>
        <actions>
            <name>EcoSetDistributionRuleToCostWork</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(
ISPICKVAL(DistributionRule__c,&quot;&quot;),
ISCHANGED(ContractTypes__c),
NOT(ISPICKVAL(ContractTypes__c,&quot;T&amp;M (with Cap)&quot;)),
NOT(ISPICKVAL(ContractTypes__c,&quot;AMPS - T&amp;M&quot;)),
NOT(ISPICKVAL(ContractTypes__c,&quot;T&amp;M&quot;)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
