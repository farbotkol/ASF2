    <rules>
        <fullName>EcoProject - Set Oracle Close Date</fullName>
        <actions>
            <name>Project_Set_Oracle_Close_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>pse__Proj__c.pse__Stage__c</field>
