        <fullName>Update_WinDate</fullName>
        <description>Update WinDate field based on Stages</description>
        <field>WinDate__c</field>
        <formula>IF(
  OR( 
      ISPICKVAL( StageName , &quot;1 Lead&quot;),
      ISPICKVAL( StageName , &quot;1A Lead - Scout&quot;),
      ISPICKVAL( StageName , &quot;1B Lead - Engage&quot;),
      ISPICKVAL( StageName , &quot;1C Lead - Position&quot;),
      ISPICKVAL( StageName , &quot;2 Capture&quot;),
      ISPICKVAL( StageName , &quot;2A Capture - LOI&quot;),
      ISPICKVAL( StageName , &quot;2B Capture - EOI&quot;),
      ISPICKVAL( StageName , &quot;2C Capture - SOQ&quot;),
      ISPICKVAL( StageName , &quot;2D Capture - RFI&quot;),
      ISPICKVAL( StageName , &quot;2E Capture - RFP&quot;),
      ISPICKVAL( StageName , &quot;3 Submitted&quot;),
      ISPICKVAL( StageName , &quot;3A Capture - Proposal Submitted&quot;),
      ISPICKVAL( StageName , &quot;3B Capture - Short List&quot;),
      ISPICKVAL( StageName , &quot;3C Capture - Interview&quot;),
      ISPICKVAL( StageName , &quot;4 Project on hold&quot;),
      ISPICKVAL( StageName , &quot;7 Canceled by Client&quot;),
      ISPICKVAL( StageName , &quot;8 No-Go&quot;),
      ISPICKVAL( StageName , &quot;9 Lost&quot;)
    ),
 null,
  IF( AND(
       ISPICKVAL( StageName , &quot;5 SNB/Win/Award&quot;),
       OR( ISPICKVAL(PRIORVALUE(StageName), &quot;1 Lead&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;1A Lead - Scout&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;1B Lead - Engage&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;1C Lead - Position&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2 Capture&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2A Capture - LOI&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2B Capture - EOI&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2C Capture - SOQ&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2D Capture - RFI&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;2E Capture - RFP&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;3 Submitted&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;3A Capture - Proposal Submitted&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;3B Capture - Short List&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;3C Capture - Interview&quot;),
              ISPICKVAL(PRIORVALUE(StageName), &quot;4 Project on hold&quot;)
),
              OR( 
                  ISBLANK(WinDate__c),
                  ISNULL(WinDate__c)
            )
          ),
      TODAY(),
     IF( AND(
              ISPICKVAL( StageName , &quot;6 Contracted/Booked/Won&quot;),
              OR( 
                  ISBLANK(WinDate__c),
                  ISNULL(WinDate__c)
              )   
            ),
         TODAY(),
          WinDate__c 
       )
    )
 )</formula>
        <name>Update WinDate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
    </fieldUpdates>
