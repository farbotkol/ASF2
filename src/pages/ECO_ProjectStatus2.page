        $( document ).ready(function() {
             $('.activeHover').hover(function() {
              $( this ).addClass("activeButtons");
            });
            $('.activeHover').hover(function() {
                $( '.activeHover' ).removeClass("activeButtons");
                $( '.decission' ).removeClass("activeButtons");
                $( this ).addClass("activeButtons");
            });
            $('.decission').hover(function() {
                $( this ).addClass("activeButtons");
            });
            $('.decission').hover(function() {
                $( '.activeHover' ).removeClass("activeButtons");
                $( '.decission' ).removeClass("activeButtons");
