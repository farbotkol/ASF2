							<apex:outputText value="The project has a current Unbilled balance of {0,number,currency}, please ensure that all invoices have been raised and this balance is correct prior to putting the project on hold">
								<apex:param value="{!oProject.UnbilledValue__c}"/>
							</apex:outputText>	
						</apex:pageMessage>					
					</div>

					<apex:outputPanel id="sections" rendered="{!oProject.RequestedProjectStatus__c == 'Closed'}">
