	<script type="text/javascript">
		function setFocusOnLoad() {}	
		
		var j$ = jQuery.noConflict();

		j$(document).ready(function (){
			showOrHideThirdPartyDescription();
			j$("input[id$='ReviewDate']").blur();
		});
	</script>

	<script>
      $j = jQuery.noConflict();
    
      // Added a check for a specific custom object
    	//	if(window.location.href.indexOf("id=a57") != -1) {
    //$j(document).ready(function() {     
    //$j("input[name='piSubmit']").hide();
    //});
    //}
	</script>
	<script type="text/javascript">
    	$j = jQuery.noConflict();
		$(document).ready(function () {
			
			$('#spinnerModal').modal({
				show: false
			});
		});
		
		function toggleShow (thisElement) 
		{
			if (thisElement.value == 'Yes' || thisElement.value == 'True' || thisElement.value == 'Unknown') 
			{
				
				$('#spinnerModal').modal({
					show: true
				});
			}
			else
			{
				$('#spinnerModal').modal({
					show: false
				});
				
			}
		}
		
		function showOrHideThirdPartyDescription(){	
			$j('input:radio[name$=showThirdPartyDescription]').each(function(index){
				if($j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=thirdPartyDescription]').show();
				}
				else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=thirdPartyDescription]').hide();
                    $j('[id$=thirdPartyText]').val('');
				}
			});
            $j('input:radio[name$=partnerSC]').each(function(index){
				if($j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=partnerAddress]').show();
                    $j('[id$=partnerGoods]').show();
				}
				else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=partnerAddress]').hide();
                    $j('[id$=partnerText1]').val('');
                    $j('[id$=partnerGoods]').hide();
                    $j('[id$=partnerText2]').val('');
				}
			});
            $j('input:checkbox[name$=jointVenture]').each(function(index){
                //alert($j(this).val());
                if($j(this).prop('checked') && $j(this).val() == "1"){
					$j('[id$=jvName]').show();
                }
                else{ //if(!$j(this).prop('checked') $j(this).val() != 'True'){
					$j('[id$=jvName]').hide();
                    $j('[id$=jvText]').val('');
                }
			});
            $j('input:radio[name$=travel]').each(function(index){
				if($j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=travel1]').show();
                    $j('[id$=travel2]').show();
				}
				else if(!$j(this).prop('checked') && $j(this).val() == 'Yes'){
					$j('[id$=travel1]').hide();
                    $j('[id$=travelText1]').val('');
                    $j('[id$=travel2]').hide();
                    $j('[id$=travelText2]').val('');
				}
			});
			if($j('[id$=governing]').val() == 'Other'){
				$j('[id$=governingCountry]').show();
			}
			else{
				$j('[id$=governingCountry]').hide();
                $j('[id$=governingText]').val('');
			}
		}
    	
    	
		function showModal () {
			$('#spinnerModal').modal({
				show: true
			});
		}
		
		function hideModal () {
			$('#spinnerModal').modal('hide');
		}
    	
	</script>
	<apex:sectionHeader title="Export Control(s)" subtitle="Export/Import Analysis Form" />
	<apex:pageMessages id="pageMessages" />
	<div class="customHelpText">
		<div class="customHelpBody">
		Export and import regulations control transfers of goods, data and services to foreign companies, persons and destinations, including foreign subsidiaries or joint ventures. The purpose of this form is to collect relevant business data in order to prepare an export/import analysis for an opportunity or transaction, and to retain business records required by applicable export and import regulations. 
		</div>
	</div>		


	<apex:form >
		
		<apex:pageBlock >
			<apex:pageBlockButtons location="both">
				<apex:commandButton action="{!ReturnToSummary}" value="Return to Summary" immediate="true" />
				<apex:commandButton action="{!SaveRecord}" value="Save" />
                <apex:commandButton action="{!SubmitForReview}" value="Submit for Review" rendered="{!NOT(oExportControl.SubmitforReview__c)}"/>
			</apex:pageBlockButtons>
		
			<apex:pageBlockSection collapsible="false" title="Section I – Basic Information Concerning Your Transaction / Opportunity"></apex:pageBlockSection>
			<div class="customHelpText">
				<div class="customHelpBody">
					The purpose of this section is to collect basic data about you and the opportunity or transaction.
				</div>
			</div>		

			<apex:outputPanel id="all">
			<table class="list table-hover" width="100%" cellpadding="0" cellspacing="0">				
				<tr>
					<td class="cell">
						EPM ID:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.EcoSystemReferenceNumber__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						If an immediate review is needed, indicate the date:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:inputField id="ReviewDate" value="{!ExportControl__c.ImmediateReviewDate__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Title of opportunity/project (transaction):
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.ProjectLongName__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Requestor's name and title: 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.Owner.Name}" />, <apex:outputField value="{!ExportControl__c.RequesterTitle__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Requestor's telephone number: 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.RequesterTelephone__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Requestor's Geography: 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.RequesterGeography__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Requestor's Business Line: 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.RequesterBusinessLine__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Requestor's AECOM Legal Entity, i.e. AECOM Technical Services, AECOM UK Ltd: 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!ExportControl__c.RequesterLegalEntity__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						List all other AECOM Legal Entities that are/will be participating in this opportunity or transaction: 
					</td>
					<td class="cell" width="180" height="200" valign="bottom">
						<apex:inputField id="LegalEntities" value="{!ExportControl__c.OtherAECOMLegalEntities__c}" style="height:200px;width:200px"/>
					</td>
				</tr>
				</table>
				
				<apex:pageBlockSection collapsible="false" title="Section II – Parties to the Transaction"></apex:pageBlockSection>	
				<div class="customHelpText">
					<div class="customHelpBody">
						The purpose of this section is to collect basic data concerning third parties, including the Ultimate End User, our client (if not the Ultimate End User),  parties known to date and contracted by the end user/owner <br/>or our client, and known parties AECOM contracts or intends to contract, including non-technical agents and consultants.
					</div>
				</div>								
				<table class="list table-hover" width="100%" cellpadding="0" cellspacing="0">
				<tr bgcolor="#C9EDFF">
					<td class="cell bold italic" colspan="2">Final Destination</td>
				</tr>
				<tr>
					<td class="cell">
						List the final destination to which goods, services or data AECOM will send (for example, a bridge designed for a project in Singapore – the final destination is Singapore): 
					</td>
					<td class="cell" width="200" valign="bottom">
                        <apex:inputField value="{!ExportControl__c.Final_Destination_Place__c}"/>
					</td>
				</tr>
				<tr bgcolor="#C9EDFF">
					<td colspan="2">
					<span class="cell bold italic" >Ultimate End User </span>
					(The “Ultimate End User” is the ultimate party, whether or not contracted with AECOM, who will be the actual beneficiary or user of AECOM supplied goods, services or data.)
					</td>
				</tr>
				<tr>
					<td class="cell">
						Ultimate End User full legal name:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!endClientAccount.Name}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Ultimate End User address:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!endClientAccount.BillingStreet}" />
						<br/>
						<apex:outputField value="{!endClientAccount.BillingCity}" /><apex:outputText value="," rendered="{!AND(bShowUltimateEndUserAddress, IF(endClientAccount.BillingCity != null, true, false))}"/>&nbsp;<apex:outputField value="{!endClientAccount.BillingState}" />
						<br/>
						<apex:outputField value="{!endClientAccount.BillingCountry}" /><apex:outputText value="," rendered="{!AND(bShowUltimateEndUserAddress, IF(endClientAccount.BillingCountry != null, true,false))}"/>&nbsp;<apex:outputField value="{!endClientAccount.BillingPostalCode}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Ultimate End User website:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:inputField value="{!ExportControl__c.End_Client_Website__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Ultimate End User an agency, branch, department or ministry of a government?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.UltimateEndUserAgency__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Ultimate End User a government-owned or state-owned business entity?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.UltimateEndUserGovtorState__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Ultimate End User a joint venture?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.UltimateEndUserJointVenture__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Ultimate End User contracting AECOM directly?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.UltimateEndUserContracting__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr bgcolor="#C9EDFF">
					<td colspan="2">
					<span class="cell bold italic">Client if not the Ultimate End User</span>
					(If the “Ultimate End User” is not contracting AECOM directly.)</td>
				</tr>
				<tr>
					<td class="cell">
						Client's full legal name:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!billingClientAccount.Name}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Client's address:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:outputField value="{!billingClientAccount.BillingStreet}" />
						<br />
						<apex:outputField value="{!billingClientAccount.BillingCity}" /><apex:outputText value="," rendered="{!bShowClientAddress}"/>&nbsp;<apex:outputField value="{!billingClientAccount.BillingState}" />
						<br />
						<apex:outputField value="{!billingClientAccount.BillingCountry}" /><apex:outputText value="," rendered="{!bShowClientAddress}"/>&nbsp;<apex:outputField value="{!billingClientAccount.BillingPostalCode}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Client's website:
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:inputField value="{!oExportControl.Billing_Client_Website__c}" />
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Client an agency, branch, department or ministry of a government?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.ClientEndUserAgency__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Client a government-owned or state-owned business entity?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.ClientEndUserGovtorState__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr>
					<td class="cell">
						Is the Client a joint venture? 
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio value="{!ExportControl__c.ClientJointVenture__c}">
			            	<apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
				<tr bgcolor="#C9EDFF">
					<td class="cell bold italic" colspan="2">Ultimate End User or Client Third Parties</td>
				</tr>
				<tr>
					<td class="cell">
						Will the Ultimate End User or Client directly hire third parties with whom AECOM will need to exchange goods, data or services?
					</td>
					<td class="cell" width="200" valign="bottom">
						<apex:selectRadio id="showThirdPartyDescription" onchange="showOrHideThirdPartyDescription()" value="{!ExportControl__c.EndUserorClientDirectHire__c}" >
                            <apex:selectOptions value="{!lAnswerValues}"/>
			            </apex:selectRadio>
					</td>
				</tr>
                <!--<apex:actionRegion >-->
                <tr id="thirdPartyDescription">
					<td class="cell" align="right" colspan="2">
						<table style="width: 100%;">
							<tr>
								<td align="right" width="60%" valign="middle">If Yes, provide the name, address and website for each third party:</td>
								<td align="right" valign="top"><apex:inputTextarea id="thirdPartyText" value="{!ExportControl__c.EndUserorClientContactInfo__c}" style="width: 98%; height: 60px; border-left: 5px solid red; padding: 10px" /></td>
							</tr>
						</table>	
					</td>
                </tr>
                <!--</apex:actionRegion>-->
                <script>
                    $('[Id $= "clientDH"]').css('display','block');
                 </script>
				<tr bgcolor="#C9EDFF">
					<td class="cell bold italic" colspan="2">AECOM's Partners or Subcontractors</td>
				</tr>
				<tr>
					<td class="cell">
						Will AECOM use partners or subcontractors? 
					</td>
					<td class="cell" width="200" valign="bottom">
						<!--<apex:actionRegion >-->
							<apex:selectRadio id="partnerSC" value="{!ExportControl__c.PartnerOrSubcontractors__c}" onchange="showOrHideThirdPartyDescription()">
								<!--<apex:actionSupport event="onclick" rerender="partner" />-->
				            	<apex:selectOptions value="{!lAnswerValues}"/>
				            </apex:selectRadio>
			            <!--</apex:actionRegion>-->
					</td>
				</tr>
                <tr >
                    <tr id="partnerAddress">
                        <td class="cell" align="right" colspan="2">
                            <table style="width: 100%;">
                                <tr>
                                    <td align="right" width="60%" valign="middle">If Yes, provide the name, address and website for each partner or subcontractor:</td>
                                    <td align="right" valign="top"><apex:inputTextarea id="partnerText1"   value="{!ExportControl__c.PartnerorSubContactInfo__c}" style="width: 98%; height: 60px; border-left: 5px solid red; padding: 10px" /></td>
                                </tr>
                            </table>
                            
                        </td>
                    </tr>
                    <tr id="partnerGoods">
                        <td class="cell" align="right" colspan="2">
                            <table style="width: 100%;">
                                <tr>
                                    <td align="right" width="60%" valign="middle">If Yes, briefly describe the goods, services or data each partner or subcontractor <br/> will contribute to this opportunity or project:</td>
                                    <td align="right" valign="top"><apex:inputTextarea id="partnerText2"   value="{!ExportControl__c.PartnerOrSubServices__c}" style="width: 98%; height: 60px; border-left: 5px solid red; padding: 10px" /></td>
                                </tr>
                            </table>
                        </td>
                    </tr>
				</tr>
				<tr bgcolor="#C9EDFF">
					<td class="cell bold italic" colspan="2">AECOM Joint Ventures</td>
				</tr>
				<tr>
					<td class="cell">
						Will AECOM be forming or use an existing Joint Venture for this opportunity or transaction?
					</td>
					<td class="cell" width="200" valign="bottom">
						<!--<apex:actionRegion >-->
							<apex:inputField id="jointVenture" value="{!oExportControl.ExistingJointVentureInput__c}" onchange="showOrHideThirdPartyDescription()">
								<!--<apex:actionSupport event="onclick" rerender="jointVenture" />-->
							</apex:inputField>
						<!--</apex:actionRegion>-->
					</td>
				</tr>
                <tr id="jvName">
					<td class="cell" align="right" colspan="2">
						<table style="width: 100%;">
							<tr>
								<td align="right" width="60%" valign="middle">If Yes, provide the name of the Joint Venture, and identify all parties in addition to AECOM who comprise this Joint Venture:</td>
								<td align="right" valign="top"><apex:inputTextarea id="jvText"  value="{!ExportControl__c.JointVentureName__c}" style="width: 98%; height: 60px; border-left: 5px solid red; padding: 10px" /></td>
							</tr>
						</table>
						
					</td>
