    <fields>
        <fullName>RiskScore__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),15,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),20,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Possible&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),25,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),30,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),40,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Likely&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),50,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Moderate&quot;)),60,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Major&quot;)),80,
IF(AND(ISPICKVAL(LikelihoodofOccurrence__c,&quot;Probable&quot;),ISPICKVAL(RiskConsequence__c,&quot;Catastrophic&quot;)),100,null)))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Risk Score</label>
        <precision>18</precision>
