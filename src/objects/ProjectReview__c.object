    <validationRules>
        <fullName>EcoHealthyStart</fullName>
        <active>false</active>
        <description>Healthy start may only be selected for C-0,C-1,C-1+,C-2 projects and projects with revenue &gt; 5M</description>
        <errorConditionFormula>AND(RecordType.Name = &quot;Healthy Start&quot;,
OR(ProjectReviewCategory__c &lt;&gt; &quot;C-0&quot;,
ProjectReviewCategory__c &lt;&gt; &quot;C-1&quot;,
ProjectReviewCategory__c &lt;&gt; &quot;C-1+&quot;,
AND(ProjectReviewCategory__c &lt;&gt; &quot;C-2&quot;, EstimatedContractValue__c &lt; 5000000)))</errorConditionFormula>
        <errorMessage>Healthy start may only be selected for C-0,C-1,C-1+,C-2 projects and projects with revenue greater than or equal to 5M</errorMessage>
    </validationRules>
