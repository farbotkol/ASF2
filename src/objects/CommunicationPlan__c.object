    <fields>
        <fullName>NewTaskNeedsToBeCreated__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND(TODAY() &gt;=  NextCreateTaskDate__c,  GenerateTask__c), true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>New Task Needs To Be Created</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NextCreateTaskDate__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISNULL( TaskLastGenerated__c ), When__c, TaskLastGenerated__c +  FrequencyInDays__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Create Task Date</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>ProjectIsActive__c</fullName>
        <externalId>false</externalId>
        <formula>AND(NOT(ISPICKVAL(Project__r.pse__Stage__c,&quot;Closed&quot;)),NOT(ISPICKVAL(Project__r.pse__Stage__c,&quot;Completed&quot;)),NOT(ISPICKVAL(Project__r.pse__Stage__c,&quot;Canceled&quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Project Is Active</label>
        <trackTrending>false</trackTrending>
