    <fields>
        <fullName>FrequencyInDays__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(1, IF(ISPICKVAL( Frequency__c, &quot;Daily&quot; ), 1, 0), 1, 
        IF(ISPICKVAL( Frequency__c, &quot;Weekly&quot; ), 1, 0), 7,
        IF(ISPICKVAL( Frequency__c, &quot;Fortnightly&quot; ), 1, 0), 14,
        IF(ISPICKVAL( Frequency__c, &quot;Monthly&quot; ), 1, 0), 30,
        IF(ISPICKVAL( Frequency__c, &quot;Quarterly&quot; ), 1, 0), 90,
        IF(ISPICKVAL( Frequency__c, &quot;Yearly&quot; ), 1, 0), 365,
        0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Frequency In Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
