    <webLinks>
        <fullName>Submit_for_Approval_Custom</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Submit Selected for Review</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/13.0/connection.js&quot;)} 
var records = {!GETRECORDIDS($ObjectType.TechnicalQualityTeam__c)}.toString().replace(/,/g , &quot;&apos;,&apos;&quot;);
var SubmitRecords = []; 
if (records[0] == null){ 
    alert(&quot;Please select at least one row.&quot;) } 
else {
     var soqlQ=&quot;SELECT Status__c, ID FROM TechnicalQualityTeam__c WHERE Id in (&apos;&quot;+records+&quot;&apos;)&quot;; 
     
      var customObj = sforce.connection.query(soqlQ);
      var soqlRes = customObj.getArray(&quot;records&quot;); 
      for(var n=0; n&lt;soqlRes.length; n++){
          if(soqlRes[n].Status__c != &quot;Submitted&quot;){
             if(soqlRes[n].Status__c != &quot;Approved&quot;){
                 SubmitRecords.push(soqlRes[n].Id);
             }
          }
      }
      if(SubmitRecords != null){
          var newRecords = [];
          for (var n=0; n&lt;SubmitRecords.length; n++) { 
             request = new sforce.ProcessSubmitRequest();
             request.objectId = trim(SubmitRecords[n]);
             request.comments=&quot;Submitting request for approval&quot;;
             newRecords.push(request); 
          } 
           var result = sforce.connection.process(newRecords); 
      }
window.location.reload(); 

}</url>
    </webLinks>
