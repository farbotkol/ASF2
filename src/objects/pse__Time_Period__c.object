    <validationRules>
        <fullName>Time_Period_Code_is_in_expected_format</fullName>
        <active>true</active>
        <description>For months: FY2015-01 for the first fiscal month in fiscal 2015. For quarters: FY2015Q1 for the first quarter in fiscal 2015. For years: FY2015 for fiscal 2015.</description>
        <errorConditionFormula>NOT(
   IF(
      text(pse__Type__c)=&apos;Month&apos;,
      AND(
         len(TimePeriodCode__c)=9,
         left(TimePeriodCode__c,2)=&apos;FY&apos;,
         isnumber(mid(TimePeriodCode__c,3,4)),
         mid(TimePeriodCode__c,7,1)=&apos;-&apos;,
         isnumber(right(TimePeriodCode__c,2))
      ),
   IF(
      text(pse__Type__c)=&apos;Quarter&apos;,
      AND(
         len(TimePeriodCode__c)=8,
         left(TimePeriodCode__c,2)=&apos;FY&apos;,
         isnumber(mid(TimePeriodCode__c,3,4)),
         mid(TimePeriodCode__c,7,1)=&apos;Q&apos;,
         isnumber(right(TimePeriodCode__c,1))
      ),
   IF(
      text(pse__Type__c)=&apos;Year&apos;,
      AND(
         len(TimePeriodCode__c)=6,
         left(TimePeriodCode__c,2)=&apos;FY&apos;,
         isnumber(mid(TimePeriodCode__c,3,4))
      ),
   TRUE)))
)</errorConditionFormula>
        <errorMessage>Time Period Code must follow this format: For months: FY2015-01 for the first fiscal month in fiscal 2015. For quarters: FY2015Q1 for the first quarter in fiscal 2015. For years: FY2015 for fiscal 2015.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Time_Period_may_not_end_before_it_starts</fullName>
        <active>true</active>
        <errorConditionFormula>pse__Start_Date__c &gt; pse__End_Date__c</errorConditionFormula>
        <errorMessage>Time Period may not end before it starts</errorMessage>
    </validationRules>
